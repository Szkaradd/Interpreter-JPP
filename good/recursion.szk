fn fib(int n) -> int {
    if (n <= 1) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fn fact(int n) -> int {
    if (n <= 1) {
        return 1;
    }
    return n * fact(n - 1);
}

fn main() -> int {
    int x = fact(6);
    print(x);
    // 720
    int y = fib(19);
    print(y);
    // 4181
}